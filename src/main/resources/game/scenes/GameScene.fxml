<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<StackPane fx:id="rootContainer" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.scenes.GameSceneController">
   <children>
      <AnchorPane fx:id="gamePane" maxHeight="720.0" maxWidth="1280.0" minHeight="720.0" minWidth="1280.0" prefHeight="720.0" prefWidth="1280.0">
         <children>
            <ImageView fitHeight="720.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../images/game_background.png" />
               </image>
            </ImageView>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#0000005c" height="680.0" layoutX="10.0" layoutY="20.0" stroke="BLACK" strokeType="INSIDE" width="1000.0" />
            <Canvas fx:id="gameCanvas" height="680.0" layoutX="10.0" layoutY="20.0" width="1000.0" />
            <Label fx:id="scoreLabel" contentDisplay="TOP" layoutX="1068.0" layoutY="70.0" prefHeight="127.0" prefWidth="166.0" stylesheets="@../fonts/GameStyle.css" text="Score:">
               <styleClass>
                  <String fx:value="text-label" />
                  <String fx:value="label-container" />
               </styleClass>
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
            <Label fx:id="scoreLabel1" contentDisplay="TOP" layoutX="1084.0" layoutY="440.0" prefHeight="102.0" prefWidth="134.0" stylesheets="@../fonts/GameStyle.css" text="Coin:">
               <styleClass>
                  <String fx:value="coin-label" />
                  <String fx:value="label-container" />
               </styleClass>
               <font>
                  <Font size="15.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</StackPane>
