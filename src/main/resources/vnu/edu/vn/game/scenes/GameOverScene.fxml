<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vnu.edu.vn.game.scenes.GameOverController">
   <children>
      <StackPane layoutX="214.0" layoutY="131.0" prefHeight="133.0" prefWidth="170.0">
         <children>
            <Label alignment="CENTER" prefHeight="145.0" prefWidth="304.0" stylesheets="@../fonts/GameStyle.css" text="Game Over" textAlignment="CENTER">
               <font>
                  <Font name="System Bold" size="57.0" />
               </font>
               <styleClass>
                  <String fx:value="score-label" />
                  <String fx:value="label-container" />
               </styleClass>
            </Label>
         </children>
      </StackPane>
      <Label fx:id="scoreLabel" alignment="CENTER" layoutX="235.0" layoutY="265.0" prefHeight="85.0" prefWidth="129.0" styleClass="score-label" stylesheets="@../fonts/GameStyle.css" text="Score: 0">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <Button fx:id="restartButton" layoutX="208.0" layoutY="400.0" mnemonicParsing="false" onAction="#setOnRestart" prefHeight="48.0" prefWidth="184.0" stylesheets="@../fonts/GameStyle.css" text="Play Again">
         <styleClass>
            <String fx:value="score-label" />
            <String fx:value="label-container" />
         </styleClass></Button>
      <Button fx:id="restartButton1" layoutX="207.0" layoutY="460.0" mnemonicParsing="false" onAction="#setToHome" prefHeight="48.0" prefWidth="184.0" stylesheets="@../fonts/GameStyle.css" text="Home">
         <styleClass>
            <String fx:value="score-label" />
            <String fx:value="label-container" />
         </styleClass>
      </Button>
      <Button fx:id="restartButton11" layoutX="207.0" layoutY="520.0" mnemonicParsing="false" onAction="#Quit" prefHeight="48.0" prefWidth="184.0" stylesheets="@../fonts/GameStyle.css" text="Quit">
         <styleClass>
            <String fx:value="score-label" />
            <String fx:value="label-container" />
         </styleClass>
      </Button>
   </children>
</AnchorPane>
